{
    "name" : "oihana/php-commands" ,
    "type" : "library" ,
    "description" : "The Oihana PHP Commands library" ,
    "keywords" : [ "php" , "commands" , "console" ] ,
    "homepage" : "https://github.com/BcommeBois/oihana-php-commands" ,
    "license" : [ "MPL-2.0" ] ,
    "authors" : [
        {
            "name" : "Marc Alcaraz" ,
            "email" : "marc@ooop.fr" ,
            "homepage" : "https://www.ooop.fr" ,
            "role" : "Project Founder, Lead Developer."
        }
    ],
    "support": {
        "email": "marc@ooop.fr"
    },
    "scripts": {
        "doc": "rm -rf docs && mkdir docs && cp -r assets docs/assets && ./vendor/bin/phpdoc -c phpdoc.xml -v" ,
        "test": "./vendor/bin/phpunit"
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\oihana\\" : "tests/oihana"
        }
    },
    "autoload" :
    {
        "psr-4" :
        {
            "oihana\\commands\\": "src/oihana/commands"
            
        },
        "files":
        [
            "src/oihana/commands/helpers/assertDomain.php" ,
            "src/oihana/commands/helpers/clearConsole.php" ,
            "src/oihana/commands/helpers/comment.php" ,
            "src/oihana/commands/helpers/domainExists.php" ,
            "src/oihana/commands/helpers/error.php" ,
            "src/oihana/commands/helpers/escapeForPrintf.php" ,
            "src/oihana/commands/helpers/format.php" ,
            "src/oihana/commands/helpers/info.php" ,
            "src/oihana/commands/helpers/makeCommand.php" ,
            "src/oihana/commands/helpers/notice.php" ,
            "src/oihana/commands/helpers/silent.php" ,
            "src/oihana/commands/helpers/success.php" ,
            "src/oihana/commands/helpers/warning.php"
        ]
    },
    "require": {
        "php": ">=8.4",
        "oihana/php-core": "dev-main",
        "oihana/php-exceptions": "dev-main",
        "oihana/php-reflect": "dev-main",
        "oihana/php-enums": "dev-main",
        "oihana/php-files": "dev-main",
        "oihana/php-system": "dev-main",
        "oihana/php-schema": "dev-main",
        "oihana/php-standards": "dev-main",
        "symfony/console": "^7.3",
        "symfony/lock": "^7.3",
        "symfony/string": "^7.3",
        "symfony/uid": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^12",
        "nunomaduro/collision": "^8.8",
        "mikey179/vfsstream": "^1.6",
        "phpdocumentor/shim": "^3.8"
    },
    "config": {
        "allow-plugins": {
            "phpdocumentor/shim": true
        }
    }
}
